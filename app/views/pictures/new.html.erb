<div class="page-header">
<h1>Add A New Picture</h1>
</div>


<%= form_tag("http://localhost:3000/create_picture") do %>
  <div>
    <%= label_tag :caption, "Caption:" %>
    <%= text_field_tag :caption %>
  </div>
  <div>
    <%= label_tag :source, "Image URL:" %>
    <%= text_field_tag :source %>
  <div>
    <input type="submit" value="Create Picture">
  </div>

<% end %>

